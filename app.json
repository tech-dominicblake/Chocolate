{
  "expo": {
    "name": "chocolate",
    "slug": "chocolate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",
    "scheme": "chocolate",
    "owner": "hushhexperience",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chocolate",
      "infoPlist": {
        "NSUsperTrackingUsageDescription": "We would like to track your activity to provide personalized content and improve your experience with the app.",
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.chocolate",
      "adaptiveIcon": {
        "foregroundImage": "./icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.479407816447-aa3frtdkhnoqoem3vf9t9eqelpmnvumv"
        }
      ],
      "expo-web-browser",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "We would like to track your activity to provide personalized content and improve your experience with the app."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./icon.png",
          "color": "#ffffff",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3fcc89b2-f1fe-48a5-bad2-944943d34388"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3fcc89b2-f1fe-48a5-bad2-944943d34388"
    }
  }
}
